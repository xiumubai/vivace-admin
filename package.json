{
  "name": "vivace-ui",
  "version": "0.1.1",
  "description": "A Vue.js 3.0 UI Toolkit for Admin",
  "files": [
    "dist"
  ],
  "main": "./dist/vivace-ui.umd.js",
  "module": "./dist/vivace-ui.mjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --config commitlint.config.js -e -V"
  },
  "config": {
    "commitizne": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "vcm"
    }
  },
  "exports": {
    ".": {
      "import": "./dist/vivace-ui.mjs",
      "require": "./dist/vivace-ui.umd.js"
    }
  },
  "homepage": "https://vivace-admin.vercel.app/",
  "keywords": [],
  "author": "xiumubai",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "2.0.2",
    "babel-eslint": "^10.1.0",
    "cz-customizable": "^6.9.1",
    "eslint": "^8.22.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "happy-dom": "6.0.4",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "terser": "^5.14.2",
    "typescript": "^4.7.4",
    "vcm-cli": "^1.0.5",
    "vite": "^3.0.7",
    "vitepress": "0.22.4",
    "vitepress-theme-demoblock": "1.4.2",
    "vitest": "0.21.1"
  },
  "dependencies": {
    "@iconify-json/ic": "^1.1.9",
    "unocss": "^0.45.6",
    "vue": "^3.2.37"
  }
}